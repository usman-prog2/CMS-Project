module.exports = 
{
    validateEmail : function(email) 
    {
        console.log(email);
        
        var params = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
        
        if(params.test(email) == false) 
        {
            return false;
        }
        
        return true;
    }

}
